<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mawkun.core.dao.InvestLogDaoExt">
        <select id="selectListByTerms" resultMap="com.mawkun.core.base.dao.InvestLogDao.InvestLogResultMap" parameterType="com.mawkun.core.base.data.query.InvestLogQuery">
            select * from invest_log
            <where>
                <if test="userId != null">and user_id = #{userId}</if>
                <if test="userName != null and userName != ''">and user_name like #{userName}</if>
                <if test="startTime != null and endTime != null">and create_time between #{startTime} and #{endTime}</if>
            </where>
        </select>
</mapper>